package ch04.function;

/**
 * @날짜:2025. 8. 22.
 * @작성자:박두식
 * @설명: 함수 = call by Value
 * 			함수 : 중복성을 없애고, 가독성이 높은 것이 중요!
 * 
 */

public class Exam51 {

	public static void main(String[] args) {
		sub(2,5);
		System.out.println("hahahah");
		System.out.println();
		
		sub(3,3);
		System.out.println("end");
		System.out.println();
		
		sub(2,10);
		System.out.println("hohoho");
		System.out.println();
	}
	public static void sub(int x, int y) { //재사용
		for(int i=0; i<x;i++) {
			for(int j=0;j<y;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
