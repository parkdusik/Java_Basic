		char gread='B';
		String level="회원등급";
		
		//프로그램 작성
		
		if(greed == 'A' || greed == 'a') {
			level="우수회원";
		}else if (greed == 'B' || 'b') {
			level="정회원";
		}else if (greed == 'C' || 'c') {
			level="일반회원";
		}else{
			level="손님";
		}
		System.out.println(level+"입니다.");
	}

}
