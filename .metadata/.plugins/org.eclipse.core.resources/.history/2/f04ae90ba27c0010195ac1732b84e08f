package ch02.control;
/*
 * 제어문 - 조건문(if, swich ~case) / 반복문(for, while, do~while)
 * 		- 조건문(if)
 * 		- if(조건식){
 * 			문장들
 * 			}
 * 
 * */

public class Exam10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x=20;
		if(x>10) { //20>10 if(1)
			System.out.printrln("10보다큰수입니다.");
		}
		
		System.out.println();
		
		int y=110;
		
		if(y>0&&y<100) {
			y+=200;
			System.out.println(y);
		}
		
	}

}
