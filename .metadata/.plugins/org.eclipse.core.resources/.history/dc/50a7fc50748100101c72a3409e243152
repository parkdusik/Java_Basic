package ch05.object;

/**
 * @날짜:2025. 8. 25.
 * @작성자:박두식
 * @설명:생성자
 */
class TestA{
	//접근 제한자 지정예약어(static, final, 생략) 반환명 함수명(매개변수 목록);
	//생성자:접근제한자 생성자명은 클래스명과 동일(매개변수 목록)
	
	public TestA() {//default 생성자
		System.out.println("hi");
	}
}

class TestB{
	
}


public class Exam70 { //실행 클래스만 public =>1개의 Java파일에 여러개의 클래스를 사용할때.

	public static void main(String[] args) {
		TestA a=new TestA(); // a객체 생성되었다. 내부적으로 생성자 생김
		TestB b=new TestB(); // b객체 생성되었다. 내부적으로 생성자 생김
	}

}
