public class Exam18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int score=84;
		String grade="";
		
		if(score>=90) {
			if(score>=97) {//100~97
				grade="A+";
			}else if(score>=94&&score<=96) { //94~96
				grade="AO";
			}else if(score<=93) {//93~90
				grade="A-";
			}
			
		}else if(score>=80) {
			
		}else if(score>=70) {
			
		}else if(score<70) {
			grade="재수강!!!";
		}
		
	}

}
