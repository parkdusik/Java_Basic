package ch05.object;

public class Test {
	private int id;
	private String name;
	private String phone;
	private int age;
	private float ki;

	
	//기본 생성자 하나
	public Test() {};
	//매개 변수가 있는 생성자 하나
	public Test(int id, String name, String phone, int age, float ki) {
		super();
		this.id = id;
		this.name = name;
		this.phone = phone;
		this.age = age;
		this.ki = ki;
	}

	
	//set//get 함수
}
