package ch04.function;
/**
 * @날짜:2025. 8. 22.
 * @작성자:박두식
 * @설명: 변수 종류
 * 		-final 상수 //변경이 안되는 상수
 * 
 */

public class Exam61 {
	public static final int VALUE=99;
	public static void main(String[] args) {
		final int SU=77;
		//SU=88; ERROR
		System.out.println(SU);
		System.out.println(VALUE);
	}
	
	public final void aa() {}
	public static final void bb() {}
	public void cc() {}
}