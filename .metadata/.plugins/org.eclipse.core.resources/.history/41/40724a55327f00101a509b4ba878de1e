package ch03.quiz_array;

public class ch03_Quiz_06반복문문제_08_03 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	double sum=0.0;
	double array[]= {1.0, 2.3, 3.4, 5.5};
	
	int i=0;
	do{
		sum+=array[i];
		i++;
	}while(i<array.length);
	System.out.println("sum:"+sum);
	}
}
