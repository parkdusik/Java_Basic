package ch05.object;

/**
 * @날짜:2025. 8. 25.
 * @작성자:박두식
 * @설명:생성자 중복
 */

class AA{
	private int x;
	private int y;
	
	public AA name(int x, int y) {
		this.x=x;
		this.y=y;
	}
	
	public void disp() {
		System.out.println(x+"\t"+y);
	}
}

public class Exam74 {

	public static void main(String[] args) {
		AA a=new AA(10, 20);
		a.disp();
		

	}

}
