package ch04.function;
/**
 * @날짜:2025. 8. 22.
 * @작성자:박두식
 * @설명: 함수 - 함수 중복-function Overload(Overloeading)
 * 		동일한 기능의 함수 명은 같게 한다.		
 * 		함수 이름은 같지만 매개변수 타입과 갯수가 달라야 한다.
 * 
 */

public class Exam58 {

	public static void main(String[] args) {
		int a=10;
		int b=20;
		int c=yonsan(a,b);
		
		float x=30.4f;
		float y=57.4f;
		float z=yonsan(x,4);
	}
	public static int yonsan(int a, int b) {
		return a+b;
	}
}