package ch06.inheritance;
/**
 * @날짜:2025. 8. 26.
 * @작성자:박두식
 * @설명:생성자 - 상속
 */

class BParents{
	protected int x;
	protected int y;
	
	public BParents() {
		System.out.println("부모 기본 생성자");
	}

	public BParents(int x) {
		this.x = x;
		this.y = 20;
	}
	
	public BParents(int x, int y) {
		this.x = x;
		this.y = y;
	}
	
	
}

public class Exam85 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
