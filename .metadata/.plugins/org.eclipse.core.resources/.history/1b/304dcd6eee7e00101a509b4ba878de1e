package ch04.function;

/**
 * @날짜:2025. 8. 22.
 * @작성자:박두식
 * @설명: 함수 = call by Value
 * 		함수 자료 받기(자료형을 맞춰야함)
 * 
 */

public class Exam50 {

	public static void main(String[] args) {
		sub(2,5);
		System.out.println("hahahaha");
		System.out.println()
		
		sub(3,3);
	}
	public static void sub(int x, int y) {
		for(int i=0; i<x;i++) {
			for(int j=0;j<y;j++) {
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
