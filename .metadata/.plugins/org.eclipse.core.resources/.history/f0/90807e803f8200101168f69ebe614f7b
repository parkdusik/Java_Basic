 * @설명:클래스 - 상속
 */

class TV{
	protected boolean power; //전원 상태
	protected int channel;
	
	public void power(Boolean power) {
		this.power=power;
	}
	
	public void chanerUp() {
		++channel;
	}
	
	public void chanerDown() {
		--channel;
	}
}

public class Exam84 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
