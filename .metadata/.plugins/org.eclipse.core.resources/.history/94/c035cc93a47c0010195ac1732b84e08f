package ch02.control;
/*
 * 제어문 - 조건문(if, swich ~case) / 반복문(for, while, do~while)
 * 		- 조건문(if)
 * 		- if(조건식){
 * 			문장들
 * 			}
 * 
 * */

public class Exam10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x=20;
		if(x>10) { //20>10 if(1)
			System.out.printrln("10보다큰수입니다.");
		}
		
		System.out.println();
		
		int y=110;
		
		if(y>0&&y<100) {	//110>0 1 && 0 110<100
			//&& 두 조건 모두 참이어야 실행되는 논리 연산자.
			// y>0이므로 참. y가 100보다 크므로 거짓 그러므로 실행이 안됨.
			y+=200;
			System.out.println(y);
		}
		
	}

}
