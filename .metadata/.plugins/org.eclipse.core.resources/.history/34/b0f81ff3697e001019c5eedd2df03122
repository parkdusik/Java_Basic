package ch02.quiz_if;
/*
 9. 키보드로부터 정수3개를 입력 받아서 a, b, c 에 저장한 후 가장 큰 값을 출력하세요. 
 */

import java.util.Scanner;

public class ch02_Quiz_05제어문문제_09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner sc=new Scanner(System.in);
		
		System.out.println("정수3개를 입력시 가장 큰 값을 출력합니다. 숫자를 입력해주세요.");
		System.out.println("첫번째 정수:");
		int a=sc.nextInt();
		System.out.println("두번째 정수:");
		int b=sc.nextInt();
		System.out.println("세번째 정수:");
		int c=sc.nextInt();
		
		
		
		
		
		
		
		sc.close();
	}
	
}
