public class Exam27 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//초기화, 조건값, 증감값 
		int x=1;
		while(x<=10) {
			System.out.println(x);
			++x;
		}
		System.out.println();
		
		int sum=0;
		int y=1;
		
	}

}
