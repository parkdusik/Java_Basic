package ch04.function;
/**
 * @날짜:2025. 8. 22.
 * @작성자:박두식
 * @설명: 함수 - 배열 사용
 * 
 */


public class Exam54 {

	public static void main(String[] args) {
	
		int[] a=new int[] {10, 20, 30, 40, 50};
		sub1(a);
		
		String[] b=new String[] {"kim","park","lee"};
		sub2(b);
		}
	
	public static void sub1(int[] a) {
		
		for(int i=0; i<a.length;i++) {
			System.out.print(a[i]+"\t");
		}
	}
}
