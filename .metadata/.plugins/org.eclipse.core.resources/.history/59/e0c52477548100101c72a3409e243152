package ch05.object;

public class Pay {
	public String name;
	public int bongbong; //왕눈이
	public int tex; // 20000000
	public int silsu;
	
	//name=n=왕눈이, bongbong=b=20000000
	public void input(String n, int b) {
		name=n;
		bongbong=b;
	}
	
	public void yonsan() {
		tex=(int)(bongbong*0.045+0.05);
		silsu=bongbong
	}
}
