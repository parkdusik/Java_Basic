package ch04.function;

/**
 * @날짜:2025. 8. 21.
 * @작성자:박두식
 * @설명: 함수 = call by Value
 * 		함수 자료 받기(자료형을 맞춰야함)
 * 
 */

public class Exam49 {

	public static void main(String[] args) {
	
		sub1(10);
		sub2(10, 20);
	}
	
	public static void sub1(int x) {
		System.out.println(x);
	}
	public static void sub2(int a, int b) {
		System.out.println("b:"+b);
	}
}
