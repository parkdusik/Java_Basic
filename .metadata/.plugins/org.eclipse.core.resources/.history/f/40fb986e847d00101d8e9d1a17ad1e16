package ch02.control;

/**
 * @날짜:2025. 8. 20.
 * @작성자:박두식
 * @설명:제어문 - 조건문(if, switch ~case) / 반복문(for, while, do~while)
 * 		제어문 - 반복문(while, do~while)
 * 		제어문 - 반복문(do~while)
 */

public class Exam28 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int x=1;
		
		do {
			System.out.print(x+"\t");
			++x;//증감값
		}while(x <=10);//조건값
		System.out.println();
	}

}
