package ch02.quiz_if;

public class ch02_Quiz_04반복문문제_11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int sum=0;
		int sum2=0;
		
		for(int i=1; i<=20; i++) {
			if(!((i%2==0)||(i%3==0))) {
				sum+=i;
			}
			if(!((i%2==0)&&(i%3==0))) {
				sum2+=i;
			}
		}
		System.out.println("sum:"+sum);
	}
}
