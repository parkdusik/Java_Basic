	String kor=sc.next();
	
	System.out.println("eng:");
	String eng=sc.next();
	
	System.out.println("mat:");
	String mat=sc.next();
	
	System.out.println("이름:" +name);
	
	total(kor, eng, mat);	
	sc.close();
	}
}
