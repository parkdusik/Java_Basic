package ch02.control;

/**
 * @날짜:2025. 8. 20.
 * @작성자:박두식
 * @설명:제어문 - break, continue
 */

public class Exam31 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		for(int i=0;i<3;i++) {		// 0   1   2 
			for(int j=0;j<5;j++) {  //012 012 012 
				if(j==3) {
					break;
				}
				System.out.println(i+"\t"+j);
			}
			System.out.println();
		}
		System.out.print("\n");
	}
}
