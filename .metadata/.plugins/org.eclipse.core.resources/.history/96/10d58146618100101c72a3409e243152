package ch05.object;

public class MyData {
	private int x; //10;
	private int y; //5;

	public void setData(int a, int y) { //a=10, b=5
		this.x=x;
		this.y=y;
	}
	
	public void disp() {
		System.out.println(x+"\t"+y);
	}
	
	public void aa() {}; //x,y
	public void bb() {}; //x,y
	public void cc() {}; //x,y
}
