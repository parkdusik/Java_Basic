package ch02.quiz_if;

public class ch02_Quiz_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String position="과장";
		String salary="직급";
		
		
		switch(position) {
		case "부장":
			salary = "700만원";
			break;
		case "과장":
			salary= "500만원";
			break;
			
		case "대리":
			salary= "300만원";
			break;
		case "사원":
			salary= "200만원";
			break;
		}
		
		System.out.println(salary);
		
	}

}
