package ch05.object;

public class Sung { //재사용 + 확장
	//속성 - 변수
	public String name;
	public int kor;
	public int eng;
	public int mat;
	public int tot;
	public float avg;
	
	//동작 - 함수
	public void total() {
		tot=kor+eng+mat;
	}
	
	public void average() {
		avg=(float)tot/3;
	}
	//학점, 등수 ....
}

	//a.name=펭수
	//a.kor=70
	//a.eng=80
	//a.mat=77
