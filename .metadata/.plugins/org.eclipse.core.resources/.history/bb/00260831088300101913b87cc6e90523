package bb;

import aa.AA; //import 삽입시 가져올 수 있음

public class DD {

	public static void main(String[] args) {
		AA a=new AA(); //패키지가 달라서 못가져옴
		System.out.println(a.x);//private //Error
		System.out.println(a.y);//protected //Error
		System.out.println(a.z); //public
		System.out.println(a.su);//default //Error
		
	}

}
