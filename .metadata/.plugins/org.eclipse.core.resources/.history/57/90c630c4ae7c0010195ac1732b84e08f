package ch02.control;
/*
 * 제어문 - 조건문(if, swich ~case) / 반복문(for, while, do~while)
 * 		- 중첩 if문
 * */

public class Exam18 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=150;
		if(num>0) {
			if(num>200) {
				System.out.println("양수이면서 200보다 큰수입니다.");
			}else {
				System.out.println("양수이면서 200보다 작은수입니다.");
			}
			
		}else {
			System.out.println("음수입니다.");
		}
		
	}

}
