		System.out.println(a.age);
		System.out.println(a.ki);
		
		Person b=new Person();
		a.name="김길동";
		a.age=33;
		a.ki=177.7f;
		
		System.out.println(a.name);
		System.out.println(a.age);
		System.out.println(a.ki);
		
