package ch02.control;

/**
 * @날짜:2025. 8. 20.
 * @작성자:박두식
 * @설명:제어문 - break, continue
 */

public class Exam32 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		/*
		int x=1; //초기값
		while(x<=10) {//조건값 5<=10
				++x;//증감값
			if(x==5) {//5==5
				break;
				//continue; 무한재귀 while로 이동 x==5  다시 continue 다시 while로 이동.
				//****continue의 무한루프 주의할것!!
			}
			System.out.println(x);
		}
		System.out.println();
		*/
		int x=0; //초기값
		while(x<=10) {//조건값 5<=10
			++x;//증감값
			if(x==5) {//5==5
				//break;
				continue;
			}
			System.out.println(x);
		}
		System.out.println();
		
		int y=1;
		do {
			++y; //증감값
			if(y==5) {
				break;
			}
			System.out.println(y);
		}while(y<=10);//조건값
		System.out.println();
	}
}
