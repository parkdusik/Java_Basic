package ch05.quiz_object;

/*
27. 정사각형을 표현하는 Rect 클래스를 활용하여, Rect 객체 배열을 생성하고, 사용자로
부터 4개의 사각형을 main에서 입력받아 배열에 저장한 뒤, 정사각형의 넓이를 구하는
프로그램을 작성하시요.

		
		[클래스 다이어그램]
		Rect
		-width: int
		-height: int
		<<create>>+Rect(width: int, height: int)
		+getArea(): int
*/

import java.util.Scanner;

class Phone{
	private String name;
	private String tel;
	private Scanner sc=new Scanner(System.in);
	
	public Phone() {
		System.out.println("성함을 입력하세요");
		this.name=sc.next();
		System.out.println("전화번호를 입력하세요");
		this.tel=sc.next();
	}
	public String getName() {
		return name;
	}
	public String getTel() {
		return tel;
	}
	
}


public class ch05_Quiz_08클래스문제_02_27 {
	public static void main(String[] args) {
		Phone kim=new Phone();
		Phone lee=new Phone();
		System.out.println(kim.getName() + "\t" + kim.getTel());
		System.out.println(lee.getName() + "\t" + lee.getTel());

	}
}
