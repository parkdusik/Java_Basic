package ch01.dataType;

/*
 *  7. 삼항 연산자 : 조건식 ? 참 :거짓
 * 
 */

public class Exam09 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a=10;
		System.out.print(a==10 ? "Ture":"False");
		
		int b=20;
		int c=30;
		int d=b!=c ? b:c;
		System.out.println(d);
	}

}
